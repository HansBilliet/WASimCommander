// @AUTOGEN_MSG@

#pragma once

#define WSMCMND_PROJECT_NAME      "@PROJECT_NAME@"
#define WSMCMND_CLIENT_NAME       "@CLIENT_NAME@"
#define WSMCMND_SERVER_NAME       "@SERVER_NAME@"
#define WSMCMND_GUI_NAME          "@APP_GUI_NAME@"

#define WSMCMND_VER_MAJOR         @VER_MAJOR@
#define WSMCMND_VER_MINOR         @VER_MINOR@
#define WSMCMND_VER_PATCH         @VER_PATCH@
#define WSMCMND_VER_BUILD         @VER_BUILD@
// Git commit hash (top 8 bytes)
#define WSMCMND_VER_COMIT         @VER_COMIT@UL
/// Version number in 32 bit "binary coded decimal", eg. 0x01230400 = 1.23.4.0
#define WSMCMND_VERSION           @VERSION@UL
/// Possible version suffix, eg "-beta1"  (can be blank for release versions)
#define WSMCMND_VER_NAME          "@VER_NAME@"
/// Dotted version string Maj.Min.Pat.Bld, eg. "1.23.4.0"
#define WSMCMND_VERSION_STR       "@VERSION_STRING@"
/// Dotted version string with possible suffix, eg. "1.23.4.0-beta1"
#define WSMCMND_VERSION_INFO      "@VERSION_STRING@@VER_NAME@"
/// Build date & time in ISO-8601 "Zulu Time" format, UTC
#define WSMCMND_BUILD_DATE        "@BUILD_DATE@"


#define WSMCMND_PROJECT_URL       "@PROJECT_URL@"
#define WSMCMND_PROJECT_COPYRIGHT "@PROJECT_COPY@"
#define WSMCMND_PROJECT_DESCRIPT  "@PROJECT_DESC@"
#define WSMCMND_PROJECT_LICENSE   "@PROJECT_LIC@"
